RewriteEngine on
RewriteBase /

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

Redirect 301 /service_eroticheskiy/indi /service_eroticheskiy
Redirect 301 /service_eroticheskiy/massazh-dlya_muzhchin /service_eroticheskiy
Redirect 301 /service_eroticheskiy/massazh-dlya_zhenshchin /service_eroticheskiy

RewriteRule ^(.*)$ index.php